            "tsConfig": "libs/specification/feature/tsconfig.spec.json",
            "karmaConfig": "libs/specification/feature/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
