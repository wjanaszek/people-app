{
  "version": 1,
  "projects": {
    "people": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/people",
      "sourceRoot": "apps/people/src",
      "prefix": "person",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/people",
            "index": "apps/people/src/index.html",
            "main": "apps/people/src/main.ts",
            "polyfills": "apps/people/src/polyfills.ts",
            "tsConfig": "apps/people/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/people/src/favicon.ico",
              "apps/people/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/people/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/people/src/environments/environment.ts",
                  "with": "apps/people/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "people:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "people:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "people:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/people/tsconfig.app.json",
              "apps/people/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/people/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/karma:karma",
          "options": {
            "jestConfig": "apps/people/karma.conf.js",
            "tsConfig": "apps/people/tsconfig.spec.json",
            "setupFile": "apps/people/src/test-setup.ts"
          }
        }
      }
    },
    "people-e2e": {
      "root": "apps/people-e2e",
      "sourceRoot": "apps/people-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/people-e2e/cypress.json",
            "tsConfig": "apps/people-e2e/tsconfig.e2e.json",
            "devServerTarget": "people:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "people:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/people-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/people-e2e/**"
            ]
          }
        }
      }
    },
    "specification-feature": {
      "projectType": "library",
